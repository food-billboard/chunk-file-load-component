{
  "private": false,
  "name": "chunk-file-load-component",
  "description": "文件上传组件",
  "version": "0.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "cross-env BUILD_TYPE=lib father-build && cross-env BUILD_TYPE=es father-build",
    "deploy": "np",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/food-billboard/chunk-file-load"
  },
  "author": "glf",
  "keywords": [
    "chunk",
    "upload",
    "component",
    "resumable"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "es/",
    "lib/"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./jest-setup-test.js"
    ]
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.4",
    "antd": "^4.16.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "chunk-file-upload": "^1.0.8",
    "classnames": "^2.3.1",
    "dropzone": "^5.9.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "react-dropzone": "^11.3.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.4",
    "@types/lodash": "^4.14.173",
    "@types/qs": "^6.9.7",
    "@umijs/test": "^3.0.5",
    "antd": "^4.16.13",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "dumi": "^1.0.16",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "father-build": "^1.17.2",
    "gh-pages": "^3.2.3",
    "jest": "^27.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.5",
    "yorkie": "^2.0.0"
  },
  "sideEffects": false
}
